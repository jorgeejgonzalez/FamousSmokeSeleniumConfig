{
  "capabilities":
  [
    {
      "browserName": "firefox",
      "acceptSslCerts": true,
      "javascriptEnabled": true,
      "takesScreenshot": false,
      "firefox_profile": "",
      "platform": "WINDOWS",
      "maxInstances": 2,
      "cleanSession": true
    },
    {
      "browserName": "chrome",
      "maxInstances": 2,
      "platform": "WINDOWS",
      "webdriver.chrome.driver": "C:/Program Files (x86)/Google/Chrome/Application/chrome.exe"
    },
    {
      "browserName": "internet explorer",
      "maxInstances": 2,
      "platform": "WINDOWS",
      "webdriver.ie.driver": "C:/Program Files (x86)/Internet Explorer/iexplore.exe"
    }
  ],
  "configuration":
  {
    "_comment" : "This is a Selenium Grid Windows Node",
    "cleanUpCycle": 2000,
    "timeout": 30000,
    "proxy": "org.openqa.grid.selenium.proxy.WebDriverRemoteProxy",
    "port": 4444,
    "register": true,
    "hubHost": "morty.famous-smoke.com",
    "hubPort": 4444,
    "maxSessions": 6
  }
}
